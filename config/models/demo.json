{
    "components": {
        "schemas": {
            "Item": {
                "required": ["name"],
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "name": {
                        "type": "string"
                    },
                    "checked": {
                        "type": "boolean",
                        "default": false
                    },
                    "createdAt": {
                        "type": "string",
                        "format": "date-time"
                    }
                }
            },

            "DemoError": {
                "type": "object",
                "properties": {
                    "message": {
                        "type": "string"
                    },
                    "code": {
                        "type": "integer"
                    }
                }
            }
        },
        "securitySchemes": {
            "oAuth": {
                "type": "oauth2",
                "description": "Oauth 2 security",
                "flows": {
                    "password": {
                        "tokenUrl": "https://oauthorize.tk/authorize",
                        "scopes": {}
                    }
                }
            },
            "bearer": {
                "type": "http",
                "scheme": "bearer"
            }
        }
    }
}
